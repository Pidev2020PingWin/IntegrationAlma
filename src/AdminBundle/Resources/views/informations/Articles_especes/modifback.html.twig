{% extends "@Admin/Default/back.html.twig" %}
{%block body%}
    <div class="form-group">
        {{ form_start(form) }}
        <label for="exampleInputEmail1">Titre</label>
        {{ form_widget(form.titre, {'attr': {'novalidate': 'novalidate'}}) }}
        <small id="emailHelp" class="form-text text-muted"></small>
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Contenu</label>
        {{ form_widget(form.contenu, {'attr': {'novalidate': 'novalidate'}}) }}
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">image</label>
        {{ form_widget(form.imageFile, {'attr': {'novalidate': 'novalidate'}}) }}
    </div>
    <div class="form-check">
        <button type="submit" class="btn btn-primary">Modifier</button>
    </div>


    {{ form_end(form) }}
{%endblock%}